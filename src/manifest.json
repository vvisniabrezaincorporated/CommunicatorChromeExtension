{
  "manifest_version": 2,
  "name": "Extension",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "128": "128.png"
  },
  "browser_action":{
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },

  "background": {
    "page": "background.html"
  },

  "options_page": "options.html",

  "permissions": [
      "storage",
      "notifications",
      "contextMenus"
  ]
}